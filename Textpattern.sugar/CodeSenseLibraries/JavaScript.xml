<?xml version="1.0" encoding="UTF-8"?>
<codesense version="1.0">
	
	<set name="com.macrabbit.sugardev.js.root-variables">
		<behavior>
			<append-dynamic>
				<matched-suffix>(\.)|[^.]</matched-suffix>
				<transform-into>(?1::.)</transform-into>
			</append-dynamic>
		</behavior>
		<completion string="action"/>
		<completion string="context"/>
		<completion string="library"/>
	</set>
	
	<set name="com.macrabbit.sugardev.js.root-methods">
		<completion string="loadLibrary">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::('$1'\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
	</set>
	
	<set name="com.macrabbit.sugardev.js.constants">
		<completion string="CETextOptionsDefault"/>
		<completion string="CETextOptionVerbatim"/>
		<completion string="CETextOptionNormalizeIndentationLevel"/>
		<completion string="CETextOptionNormalizeLineEndingCharacters"/>
		<completion string="CETextOptionNormalizeIndentationCharacters"/>
	</set>
	
	<set name="com.macrabbit.sugardev.js.global-properties">
		<completion string="location"/>
	</set>
	
	<set name="com.macrabbit.sugardev.js.constructors">
		<completion string="SXSelector">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:selector}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		<completion string="CETextRecipe">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		<completion string="CETextSnippet">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:snippet}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		<completion string="Range">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:location}, ${2:length}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
	</set>
	
	<set name="com.macrabbit.sugardev.js.action.properties">
		<behavior>
			<append-dynamic>
				<matched-suffix>(=| +=)| *[^=]</matched-suffix>
				<transform-into>(?1:: = function(context, outError\) {
	$0
};)</transform-into>
			</append-dynamic>
		</behavior>
		<completion string="canPerformWithContext"/>
		<completion string="performWithContext"/>
		<completion string="titleWithContext"/>
		<completion string="setup">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\.)|[^.]</matched-suffix>
					<transform-into>(?1::.)</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
	</set>
	
	<set name="com.macrabbit.sugardev.js.context.properties">
		<completion string="selectedRanges"/>
		<completion string="string"/>
		<completion string="lineStorage"/>
		<completion string="syntaxTree"/>
		<completion string="itemizer"/>
		<completion string="textPreferences"/>
		<completion string="substringWithRange">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:range}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		<completion string="applyTextRecipe">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:recipe}${2:, ${3:options}}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
		<completion string="insertTextSnippet">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:snippet}${2:, ${3:options}}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
	</set>
	
